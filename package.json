{
  "name": "bandoneon",
  "version": "3.5.1",
  "description": "A little JavaScript application that wants to help learning the Alfred Arnold 142 bandoneon.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicokaiser/bandoneon.git"
  },
  "author": "Nico Kaiser",
  "license": "MIT",
  "homepage": "https://bandoneon.app",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "prettier": "prettier src --write"
  },
  "dependencies": {
    "@tonaljs/note": "^4.10.0",
    "@tonaljs/scale": "^4.12.2",
    "bootstrap": "^5.3.1",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@types/bootstrap": "^5.2.6",
    "@types/node": "20.4.9",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.1",
    "sass": "^1.64.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended",
      "@vue/eslint-config-typescript",
      "prettier"
    ],
    "globals": {
      "defineEmits": true,
      "defineExpose": true,
      "defineProps": true
    }
  }
}
