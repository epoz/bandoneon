{
  "name": "bandoneon",
  "version": "3.5.6",
  "description": "A little JavaScript application that wants to help learning the Alfred Arnold 142 bandoneon.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicokaiser/bandoneon.git"
  },
  "author": "Nico Kaiser",
  "license": "MIT",
  "homepage": "https://bandoneon.app",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "prettier": "prettier src --write"
  },
  "dependencies": {
    "@tonaljs/note": "^4.10.0",
    "@tonaljs/scale": "^4.12.2",
    "bootstrap": "^5.3.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.3",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "20.10.6",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "sass": "^1.69.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.27"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended",
      "@vue/eslint-config-typescript",
      "prettier"
    ],
    "globals": {
      "defineEmits": true,
      "defineExpose": true,
      "defineProps": true
    }
  },
  "type": "module"
}
