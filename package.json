{
  "name": "bandoneon",
  "version": "2.0.0",
  "description": "A little JavaScript application that wants to help learning the Alfred Arnold 142 bandoneon.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicokaiser/bandoneon.git"
  },
  "author": "Nico Kaiser",
  "license": "MIT",
  "homepage": "https://bandoneon.app",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@tonaljs/chord": "^3.8.1",
    "@tonaljs/note": "^3.6.0",
    "@tonaljs/scale": "^3.6.1",
    "bootstrap": "^4.5.3",
    "core-js": "^3.6.5",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.1",
    "vue-localstorage": "^0.6.2",
    "vue-router": "^3.4.7",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-vue": "^7.1.0",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "vue/script-indent": [
        "warn",
        2,
        {
          "baseIndent": 1,
          "switchCase": 1
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
